{
  "name": "calculator-utility",
  "version": "0.5.4",
  "description": "🐕‍🦺 Utility functions for calculator apps.",
  "keywords": [
    "calculator",
    "utility",
    "math",
    "calculation",
    "arithmetic"
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "default": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    }
  },
  "scripts": {
    "prepare": "husky install",
    "watch": "tsup --watch",
    "build": "tsup",
    "release": "pnpm run build && pnpm publish",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint \"src/**\"",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "author": "Furkan Emin Can",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/femincan/calculator-utility"
  },
  "homepage": "https://github.com/femincan/calculator-utility#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.34.4",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "prettier": "^3.0.3",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "dist"
  ]
}
