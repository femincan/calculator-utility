{
  "name": "calculator-utility",
  "version": "0.5.4",
  "description": "ðŸ•â€ðŸ¦º Utility functions for calculator apps.",
  "keywords": [
    "calculator",
    "utility",
    "math",
    "calculation",
    "arithmetic"
  ],
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "watch": "parcel watch",
    "build": "parcel build --no-source-maps",
    "build:clean": "rm -rf .parcel-cache/ dist/",
    "release": "npm run build:clean && npm run build && npm publish",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint \"src/**\"",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "author": "Furkan Emin Can",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/femincan/calculator-utility"
  },
  "homepage": "https://github.com/femincan/calculator-utility#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.9.3",
    "@parcel/transformer-typescript-types": "^2.9.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.34.4",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "parcel": "^2.9.3",
    "parcel-resolver-ts-base-url": "^1.3.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "dist/**/*"
  ]
}
